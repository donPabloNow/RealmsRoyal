{
  "name": "RealmsRoyal",
  "description": "RealmsRoyal online multiplayer realms royal",
  "author": "don Pablo",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "pre-commit": {
    "run": "lint",
    "silent": true
  },
  "scripts": {
    "compile:protocol": "pbjs -t static-module -w es6 -o core/protocol.js --no-comments server/messages.proto",
    "lint": "eslint --cache --ext .js server",
    "start": "cross-env CLIENT=true PRELOAD=10 SEED=1980 STORAGE=./data nodemon --watch server server/main.js"
  },
  "dependencies": {
    "colorsys": "^1.0.22",
    "cors": "^2.8.5",
    "eslint-config-airbnb": "18.2.1",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "fastnoisejs": "^1.0.1",
    "helmet": "^4.1.1",
    "node-fetch": "^2.6.1",
    "pngjs": "^6.0.0",
    "protobufjs": "^6.10.1",
    "uuid": "^8.3.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint": "7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "4.0.0",
    "nodemon": "^2.0.4",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.0"
  }
